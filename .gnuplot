set grid
set hidden3d
set xyplane at 0
set pointsize 2

set style data linespoint

# Set default font for terminal
set terminal postscript eps enhanced color font "arial,20"
set terminal ggi mode V640x480
set terminal emf color enhanced font "arial,20"
set terminal pdf enhanced font "Helvetica, 20" lw 2 dashed
set terminal x11 enhanced font "arial,20"
set terminal pdf enhanced font "Helvetica, 20" lw 2 dashed
