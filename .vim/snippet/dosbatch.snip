snippet     for
abbr        for
	for ${1:option} %%${2:var} in (${3:target}) do (
		${4:cmd} %%$2
	)
	
	${0}

snippet     if
abbr        if
options     head
	if ${1:condition} (
		${2:cmd}
	)
	
	${0}

snippet     if-else
abbr        ife
options     head
	if ${1:condition} (
		${2:cmd}
	) else (
		${3:cmd}
	)
	
	${0}

